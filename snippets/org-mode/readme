* NAME

#+BEGIN_SRC lisp :exports results
(let ((system (asdf:find-system "${1:system}")))
  (format nil "~A --- ~A"
    (asdf:component-name system)
    (asdf:system-description system)))
#+END_SRC

* VERSION

#+BEGIN_SRC lisp :exports results
(format nil "Version ~A"
        (asdf:component-version (asdf:find-system "$1")))
#+END_SRC

